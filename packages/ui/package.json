{
  "name": "@douglasneuroinformatics/ui",
  "version": "6.0.0-beta.2",
  "description": "Common React components and tailwind config for DNP projects",
  "license": "LGPL-3.0",
  "type": "module",
  "exports": {
    "./components": {
      "import": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks/index.js",
      "types": "./dist/hooks/index.d.ts"
    },
    "./i18n": {
      "import": "./dist/i18n.js",
      "types": "./dist/i18n.d.ts"
    },
    "./legacy": {
      "import": "./dist/legacy/index.js",
      "types": "./dist/legacy/index.d.ts"
    },
    "./styles/*": "./dist/styles/*",
    "./tailwind.config.cjs": "./tailwind.config.cjs",
    "./utils": {
      "import": "./dist/utils.js",
      "types": "./dist/utils.d.ts"
    }
  },
  "files": [
    "dist",
    "tailwind.config.cjs",
    "tailwind.config.d.cts"
  ],
  "scripts": {
    "build": "tsup && cp -r src/styles dist",
    "format": "prettier --write src",
    "lint": "tsc && eslint --fix src",
    "push": "pnpm lint && pnpm build && npm publish",
    "storybook": "storybook dev -p 6006",
    "test": "vitest --run"
  },
  "dependencies": {
    "@douglasneuroinformatics/form-types": "^5.2.0",
    "@douglasneuroinformatics/utils": "^1.2.2",
    "@headlessui/react": "^1.7.17",
    "@headlessui/tailwindcss": "^0.2.0",
    "@heroicons/react": "^2.0.18",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.4",
    "i18next": "^23.6.0",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.3.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react-i18next": "^13.3.1",
    "recharts": "^2.9.0",
    "tailwind-merge": "^1.14.0",
    "ts-pattern": "^5.0.5",
    "type-fest": "^4.6.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.4"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.5.1",
    "@storybook/addon-interactions": "^7.5.1",
    "@storybook/addon-links": "^7.5.1",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^7.5.1",
    "@storybook/react": "^7.5.1",
    "@storybook/react-vite": "^7.5.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/lodash": "^4.14.200",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "autoprefixer": "^10.4.16",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "happy-dom": "^13.3.8",
    "postcss": "^8.4.31",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.5.1",
    "storybook-react-i18next": "^2.0.9",
    "tailwindcss": "^3.3.5",
    "tailwindcss-animate": "^1.0.7",
    "vite": "^4.5.0"
  },
  "peerDependencies": {
    "@headlessui/tailwindcss": "^0.2.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "react": ">=18",
    "react-dom": ">=18",
    "tailwindcss": ">=3"
  },
  "publishConfig": {
    "access": "public"
  }
}
